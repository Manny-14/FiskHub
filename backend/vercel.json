{
    "version" : 2,
    "name" : "fisk-hub",
    "builds" : [
        {
            "src" : "./index.js",
            "use" : "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "/",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": [
                {
                    "source": "/(.*)",
                    "headers": [
                    { "key": "Access-Control-Allow-Origin", "value": "*" }
                    ]
                },
                {
                    "source": "/https://fiskhub.vercel.app/(.*)",
                    "headers": [
                    { "key": "Access-Control-Allow-Origin", "value": "*" }
                    ]
                }  
            ]
        }
    ]
}