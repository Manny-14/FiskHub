{
    "version" : 2,
    "name" : "fisk-hub",
    "builds" : [
        {
            "src" : "./index.js",
            "use" : "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "/",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": 
                {
                "source": "/api/(.*)",
                "headers": [
                    { "key": "Access-Control-Allow-Origin", "value": "https://fiskhub.vercel.app" },
                    { "key": "Access-Control-Allow-Methods", "value": "GET, POST, OPTIONS" }
                ]
                }
        
        }
    ]
}